! DPI seting
Xft.antialias:  true
Xft.autohint:   0
Xft.hinting:    true
Xft.hintstyle:  hintslight
Xft.lcdfilter:  lcddefault
Xft.rgba:       rgb
Xft.dpi:        96
! Xft.dpi:        156

! special
*.foreground:   #eeffff
*.background:   #1e2541
*.cursorColor:  #eeffff

! black
*.color0:       #1e2541
