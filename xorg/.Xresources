! red
*.color1:       #f0719b
*.color9:       #f02e6e

! green
*.color2:       #5af7b0
*.color10:      #2ce592

! yellow
*.color3:       #ffa56b
*.color11:      #ff8537

! blue
*.color4:       #57c7ff
*.color12:      #1da0e2

! magenta
*.color5:       #c792ea
*.color13:      #a742ea

! cyan
*.color6:       #89ddff
*.color14:      #47bae8

! white
*.color7:       #eeffff
*.color15:      #dee6e7

